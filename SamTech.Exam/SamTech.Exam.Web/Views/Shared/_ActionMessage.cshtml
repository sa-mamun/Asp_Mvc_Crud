@model ResponseModel
@if (Model != null)
{
    <div class="alert @Model.IconCssClass alert-dismissible">
        <button type="button" class="close" data-dismiss="alert" aria-hidden="true">&times;</button>
        <h5><i class="icon fas @Model.StyleCssClass"></i> @Model.Title!</h5>
        @Model.Message
    </div>
}
